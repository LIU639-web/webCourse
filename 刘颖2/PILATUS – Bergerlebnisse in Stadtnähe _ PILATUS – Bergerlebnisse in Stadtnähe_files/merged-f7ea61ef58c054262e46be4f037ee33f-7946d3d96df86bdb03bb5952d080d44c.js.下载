    $('.filter-navigation').find('a').click(function(e){
		e.preventDefault();
		$this = $(this);
		$this.parents('.filter-navigation').find('a.active').removeClass('active');
		$this.addClass('active');
 		var filterValue = $this.attr('data-filter');
 		location.hash = 'filter=' + encodeURIComponent( filterValue );
		$grid.isotope({ filter: filterValue });
	});

	if($('.filter-navigation').length > 0){
		var hashFilter = getHashFilter();
	    if ( !hashFilter ) {
	      return;
	    }
	    // filter isotope
	   var $grid = $('.teaser-grid').isotope({
			itemSelector: '.teaser-grid-item',
		  	percentPosition: true,

		  	masonry: {
		    	columnWidth: ($('.teaser-grid-item.columns.medium-6.large-3').length > 0 ? '.teaser-grid-item.columns.medium-6.large-3' : '.teaser-grid-item.columns.medium-6')
		  	},
		  	filter: hashFilter
		})
	    // set selected class on button
	    if ( hashFilter ) {
	    	$('.filter-navigation').find('.active').removeClass('active');
	      	$('.filter-navigation').find('[data-filter="' + hashFilter + '"]').addClass('active');
	    }
	}

	$('#main-navigation, #mobile-menu').find('a.p-14, a.p-15').click(function(e){
		e.preventDefault();
		var toURL = $(this).attr('href');
		window.location = toURL;
		if(toURL == location.href){
			location.reload();
		}
	})

	// to Top Link
	$('#totop').click(function(e){
		e.preventDefault();
		TweenLite.to(window, 1, {scrollTo:0, ease:Power2.easeOut});
	})
	// tp Down Link
	$('#scroll-down-link').click(function(e){
		e.preventDefault();
		TweenLite.to(window, 1, {scrollTo: $('#main-content').position().top, ease:Power2.easeOut});
	})

