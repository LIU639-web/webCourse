  	$('.header-border-top, .animate-trigger, .activities-slider-container, .header-border-bottom-container, .icon-content-container, .block-grid-teaser, .header-mood, .image-text, .highlight-container, .teaser-one-col, .dragon-separator, .header-content').each(function (index, elem) {
