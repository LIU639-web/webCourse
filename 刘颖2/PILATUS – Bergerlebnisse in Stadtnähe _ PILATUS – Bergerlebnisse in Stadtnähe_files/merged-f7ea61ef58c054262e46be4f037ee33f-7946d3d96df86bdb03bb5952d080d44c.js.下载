	$('.iframe-loader').click(function(e){
		e.preventDefault();
		var $this = $(this);
		$this.prev().attr('src', $this.prev().data('src'));
		$this.hide();
		$this.parents('.tabs-content').find('.tab-thumb-item.first-item').addClass('active');
	})

	$('.tab-content-360 .tab-thumb-item').click(function(e){
		e.preventDefault();
		$this = $(this);
		$this.parents('.tab-thumbs').slick('slickGoTo', $this.data('slick-index'));
		$('.tab-content-360 .tab-thumb-item')
		$this.parents('.tab-content-360').find('.iframe-loader').hide();
		var iframeSource = $this.data('iframe');
		$('#tab-content-iframe').attr('src', iframeSource);
		$this.parent().find('.active').removeClass('active');
		$this.addClass('active');
	})

	$('.tab-content-videos .tab-thumb-item').click(function(e){
		e.preventDefault();
		$this = $(this);
		$this.parents('.tab-thumbs').slick('slickGoTo', $this.data('slick-index'));
		$this.parents('.tab-content-videos').find('.iframe-loader').hide();
		var ytSource = $this.data('iframe');
		$('#yt-iframe').attr('src', ytSource);
		$this.parent().find('.active').removeClass('active');
		$this.addClass('active');
	})

