	});

	// TABS
	$('#media-tabs').on('change.zf.tabs', function() {

		$('#yt-iframe').each(function(){
			this.contentWindow.postMessage('{"event":"command","func":"' + 'stopVideo' + '","args":""}', '*')
		});
	});

	$('#home-media-tabs .slick-arrow').click(function(e){
		var $mediaTabsContainer = $('#media-tabs-container');
		var $actTab = $mediaTabsContainer.find('.tabs-title.is-active');
		var nextTab = '';


		if( $(this).hasClass('icon-arrow-left') ){
			if( $actTab.is(':first-child') ){
				nextTab = $mediaTabsContainer.find('li.tabs-title:last-child a').attr('href');
			}else{
				nextTab = $actTab.prev().find('a').attr('href');
			}
		}else{
			if( $actTab.is(':last-child') ){
				nextTab = $mediaTabsContainer.find('li.tabs-title:first-child a').attr('href');
			}else{
				nextTab = $actTab.next().find('a').attr('href');
			}
		}

		console.log(nextTab);
		$('#media-tabs').foundation('selectTab', nextTab.substring(1) ,false);
	})

	// IMAGES LOADED
	$('#content-mood').imagesLoaded({ background: true }).done( function( instance ) {
 		$('#content-mood').addClass('ready');
	});

	// MMENU
	if(!$('body').hasClass('loading-content')){
		var $menu = $("#mobile-menu").mmenu({
			"extensions": [
				"pagedim-black"
			],
			"offCanvas": {
				zposition: "front",
				pageSelector: "#page-wrapper",
           		"position": "right"
			},
			navbar: {
		    	title: "Navigation"
		  	}},{
				classNames: {
					// fixedElements: {
					// 	fixed: "fixed-fix"
					// }
				}
			}
		);
		var $icon = $("#hamburger");
		var API = $menu.data( "mmenu" );

		$icon.on( "click", function() {
			if( $(this).hasClass('is-active') ){
				API.close();
			}else{
				API.open();
			}
		});
		API.bind( "opened", function() {
			$icon.addClass( "is-active" );
		});
		API.bind( "closed", function() {
			$icon.removeClass( "is-active" );
		});

	}

	// FOUNDATION
	$(document).foundation();

	// ACCORDION
	if( Foundation.MediaQuery.atLeast('medium') ){
		$('.live-accordion-item > a').off('click.zf.accordion');
	}

	$(window).on('changed.zf.mediaquery', function(event, newSize, oldSize){
		if($('.live-accordion-item').length > 0){
		    if((newSize == 'large' || newSize == 'xlarge' || newSize == 'xxlarge' || newSize == 'medium') && oldSize == 'small'){
				$('.live-accordion-item > a').off('click.zf.accordion');
		    }else if((oldSize == 'large' || oldSize == 'xlarge' || oldSize == 'xxlarge' || oldSize == 'medium') && newSize == 'small'){
		    	Foundation.reInit(['accordion']);
		    }
		}
	});

	// YOUTUBE AUTOPLAY
	$(document).on('closed.zf.reveal', '[data-reveal]', function (e) {
		var $this = $(this);
		if( $this.hasClass('reveal-video') ){
			$this.find('iframe').attr('src', $this.find('iframe').attr('src').replace('autoplay=1', 'autoplay=0'));
		}
	});
	$(document).on('open.zf.reveal', '[data-reveal]', function (e) {
		var $this = $(this);
		if( $this.hasClass('reveal-video') ){
			$this.find('iframe').attr('src', $this.find('iframe').attr('src').replace('autoplay=0', 'autoplay=1'));
		}
	});

	// SLICK MOOD
	$('#mood-slider').on('init', function(event, slick){
		setTimeout(function(){
			removeLoader();

			setTimeout(function(){
				$(slick.$slides[0]).addClass('slick-animating-in');

				var $currentSlider = $(slick.$slides[0]);
				var $this = $('#mood-slider');

				if( $currentSlider.hasClass('mood-slider-item-video') && Foundation.MediaQuery.atLeast('medium') ){

					setTimeout(function(){
						if( $currentSlider.hasClass('mood-slider-item-video') ){
							$this.slick('slickPause');
							var video = $currentSlider.find('video')[0];

							video.play();
							$currentSlider.addClass('show-video');

							video.addEventListener('ended',function(){
								$this.slick('slickNext');
								$this.slick('slickPlay');
							},false);

						}
					}, 2000);

				}

			}, 500);

			if( $('#mood-slider').length > 0){
				if (window.DeviceOrientationEvent && Modernizr.touchevents) {
					var parallax = new moodparallaxmobile();
				}else{
			    	var parallax = new moodparallax();
				}
			}
		}, 500);
	});
	$('#mood-slider').on('beforeChange', function(event, slick, currentSlide, nextSlide){
		$(slick.$slides[nextSlide]).removeClass('slick-animating-in');
	});
	$('#mood-slider').on('afterChange', function(event, slick, currentSlide){
		var $currentSlider = $(slick.$slides[currentSlide]);
		var $this = $(this);

		$currentSlider.addClass('slick-animating-in');

		if( $currentSlider.hasClass('mood-slider-item-video') ){
			$this.slick('slickPause');
			var video = $currentSlider.find('video')[0];
			video.pause();
			video.currentTime = 0;

			setTimeout(function(){
				if( $currentSlider.hasClass('mood-slider-item-video') && Foundation.MediaQuery.atLeast('medium') ){
					video.play();

					$currentSlider.addClass('show-video');
					video.addEventListener('ended',function(){
						$this.slick('slickNext');
						$this.slick('slickPlay');
					},false);
				}
			}, 1800);
		}else{
			$('.mood-slider-item-video').each(function(e){
				$('.mood-slider-item-video').removeClass('show-video');
				var eachVideo = $(this).find('video')[0];
				eachVideo.pause();
			})
		}
	});

	$('#mood-slider').slick({
		dots: false,
		infinite: true,
		speed: 500,
		cssEase: 'cubic-bezier(0.250, 0.250, 0.055, 1.000)',
		prevArrow: '<button class="icon-arrow-left slick-arrow" />',
		nextArrow: '<button class="icon-arrow-right slick-arrow" />',
		swipe: true,
		draggable: true,
		autoplay: true,
		autoplaySpeed: 10000,
		pauseOnHover: false
	});

