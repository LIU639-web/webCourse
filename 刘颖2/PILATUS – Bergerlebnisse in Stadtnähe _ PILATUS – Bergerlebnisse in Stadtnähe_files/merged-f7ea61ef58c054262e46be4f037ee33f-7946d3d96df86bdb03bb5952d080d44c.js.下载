function getImportantMessage(){
	var lang = $('html').attr('lang');
	var langid = 0;
	var langFlag = 'de';
    switch(lang){
		case 'de':
		case 'de-CH':
			langid = 0;
			langFlag = 'de';
			break;
        case 'fr':
        case 'fr-CH':
            langid = 1;
			langFlag = 'fr';
            break;
        case 'it':
        case 'it-CH':
            langid = 2;
			langFlag = 'it';
            break;
		case 'en':
		case 'en-GB':
			langid = 3;
			langFlag = 'en';
            break;
		default:
			langid = 0;
			langFlag = 'de';
	}
    /*var url = (lang != 'de')? "/" + lang : '';
    console.log(url);
    url += +'/index.php?type=6010&tx_mxntemplate_ajax[controller]=Ajax&tx_mxntemplate_ajax[action]=getMessage&L='+langid;*/


    if(langFlag == 'de'){
		var url = 'https://www.pilatus.ch/index.php?type=6010&tx_mxntemplate_ajax[controller]=Ajax&tx_mxntemplate_ajax[action]=getMessage&L='+langid;
	} else {
		var url = "https://www.pilatus.ch/" + langFlag + '/index.php?type=6010&tx_mxntemplate_ajax[controller]=Ajax&tx_mxntemplate_ajax[action]=getMessage&L='+langid;
	}

/*

* */


	//var url = "/"+(lang != 'de')? lang : ''+'/?type=6010';
    $.ajax({
        type: "POST",
        url: url,
        success: function (data) {


            if(data.length > 0){

				$('#important-message-container').html(data);

                // COOKIE
                var acutalTopPosition = 0;
                var messageHeight = 0;
                $('#close-important-message').on('click', function(){
                    setCookie('close_important_message', 'true', 4);
                    $('body').removeClass('show-important-message');
                    $('header').css('top', acutalTopPosition+'px');
                    setFullHeight();
                });

                if( $('#important-message').length > 0 ){
                    if(!getCookie('close_important_message')){
                        $('body').addClass('show-important-message');
                        messageHeight = parseInt($('#important-message').outerHeight());
                        acutalTopPosition = parseInt($('header').css('top'));
                        $('header').css('top', acutalTopPosition+messageHeight+'px');
                        setFullHeight();
                    }
                }
            }
        },
        error: function (data) {
            //console.log('error');
        },
        complete: function () {

        }
    });
}


// jQUERY
$(function() {

	function iOSversion() {

		if (/iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream) {
			if (!!window.indexedDB) { return 'iOS 8 and up'; }
			if (!!window.SpeechSynthesisUtterance) { return 'iOS 7'; }
			if (!!window.webkitAudioContext) { return 'iOS 6'; }
			if (!!window.matchMedia) { return 'iOS 5'; }
			if (!!window.history && 'pushState' in window.history) { return 'iOS 4'; }
			return 'iOS 3 or earlier';
		}

		return 'Not an iOS device';
	}


		$('.entry').click(function(){
			if($( window ).width() < 639){

			/*if($(this).data('url') != ''){

				window.location.href = $(this).data('url');
			}*/
			var href = $(this).find('.entry-link a').attr('href');
				window.location.href = href;
			}
		});



	$('form#filter-calendar').submit(function(e){
		e.preventDefault();
		var startDate = $('#calendar-in').val();
		if(startDate != ''){
            startDate = startDate.split(".");
            startDate = new Date(startDate[2],parseInt(startDate[1])-1,startDate[0]);
		}


		if($('#calendar-out').length){
            var endDate = $('#calendar-out').val();
            if(endDate != ''){
                endDate = endDate.split(".");
                endDate = new Date(endDate[2],parseInt(endDate[1])-1,endDate[0]);
            }
		}



		var filter = $('#search-cats').find(":selected").val();

		var filtertext = $('#calendar-searchstring').val();

		$('.tx-mxn-calendar .row .columns .entry').each(function(){
			$(this).show();
			if(filter && filter != '' && !$(this).hasClass(filter.substring(1))){
				$(this).hide();
			}
            if(startDate != ''){
				if(startDate.getTime()+3600000 > parseInt($(this).attr('data-timestamp'))*1000){
                    $(this).hide();
				}
            }
            /*if(endDate != ''){
                if(endDate.getTime()+3600000 < parseInt($(this).attr('data-timestamp'))*1000){
                    $(this).hide();
                }

            }*/

            if(filtertext != ''){

                var entryString = $(this).find('.entry-text .title').text().toLowerCase() + " " + $(this).find('.entry-text .title').attr('data-searchstring').toLowerCase();

                if(!entryString.includes(filtertext.toLowerCase())){
                    $(this).hide();
                }
            }
		});

		var months = [];
		$('.tx-mxn-calendar .month-entries-row').each(function(){
			if($(this).find('.entry').css('display') == 'block'){
                months[$(this).attr('data-month')] = true;
			}


		});

        $('.tx-mxn-calendar .month-title-row').each(function(){
        	$(this).show();

			if(months[$(this).attr('data-month')] === undefined){
				$(this).hide();
			}
        });
	});

    if($('#important-message-container').length){
        getImportantMessage();
	}


	var actLang = $('html').attr('lang');

	if( navigator.userAgent.toLowerCase().indexOf('firefox') > -1 ){
		$('body').addClass('firefox');
	}

	// IMAGES NO-CACHE (WITH DATE PARAEMTER)
	$('img.no-cache').each(function(){
		$(this).attr('src', $(this).attr('src') + '?t='+  Date.now());
	})

	// FOOTER DATA-URI
	$('#footer-container').find('[data-uri]').click(function(e){
		e.preventDefault();
		window.location = $(this).data('uri');
	});

	// MODAL LIVE TEST
	var mapPathname  = $('#go-map').data('url');
   	var livePathname = $('#go-live').data('url');

	$('#go-live, a[href*="'+livePathname+'"]').click(function(e){


		/*var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;*/
		console.log(navigator.userAgent);
		var iOS = /Version\/13.1 Safari\/605.1.15/.test(navigator.userAgent) && !window.MSStream;

		console.log(iOS);
		console.log(navigator.userAgent);
		/*console.log(iOSversion());*/

		var attr = $(this).attr('target');
		var external = false;
		if(typeof attr !== typeof undefined && attr !== false){
			if (attr == '_blank') {
				external = true;
			}
		}

		if(!external){
			e.preventDefault();
			if(is_ie9) {
				window.location = livePathname;
			} else if(iOS){
				var url = $('#go-live').data('url');
				url = "https://www.pilatus.ch/live";
				/*url = "https://www.pilatus.ch/index.php?id=177";*/
				window.location.href = url;

				if( !!(window.history && history.pushState) ){
					history.pushState({ page:  url}, 'NULL', url);
				}
				if( $('#check-live').length > 0 ){
					$('#live-modal').foundation('open');
				}else{
					var dataUrl = url;
					// loadLive(dataUrl);
					addLoaderContent('live', dataUrl, false);
				}
			}else{
				var url = $('#go-live').data('url');

				if( !!(window.history && history.pushState) ){
					history.pushState({ page:  url}, 'NULL', url);
				}
				if( $('#check-live').length > 0 ){
					$('#live-modal').foundation('open');
				}else{
					var dataUrl = url;
					// loadLive(dataUrl);
					addLoaderContent('live', dataUrl, false);
				}
			}
		}

	});
//

	$('#go-map, a[href*="'+mapPathname+'"]').click(function(e){
		var attr = $(this).attr('target');
		var external = false;
		if(typeof attr !== typeof undefined && attr !== false){
			if (attr == '_blank') {
				external = true;
			}
		}
		if(!external){


		e.preventDefault();
		if(is_ie9){
			window.location = mapPathname;
		}else{
			if( !!(window.history && history.pushState) ){
		    	history.pushState({ page: $(this).attr('href') }, 'NULL', $(this).attr('href'));
		    }

			if( $('#themap').length > 0 ){
				$('#map-modal').foundation('open');
			}else{
				var dataUrl = $('#go-map').attr('href');
				// loadMap(dataUrl);
				addLoaderContent('map', dataUrl, false);
			}
		}
		}
	});
