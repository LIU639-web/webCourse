    // TAGDUCK lOADING
	setTimeout(function(){
		$('.loading-social').each(function (index, elem) {
		    new ScrollMagic.Scene({
		        triggerElement: elem,
		        triggerHook: '0.8',
		        reverse: false
		    })
		    .on('start', function () {
		    	var $elem = $(elem);
		    	var $img = $elem.find('img');
		    	$img.attr('src', $img.data('src'));
		    	$img.imagesLoaded().done( function( instance ) {
			 		$elem.addClass('animate');
				});
		    })
		    .addTo(sm_controller)
		});
	}, 500);

	$('.loading-ajax').each(function (index, elem) {
	    new ScrollMagic.Scene({
	        triggerElement: elem,
	        triggerHook: '0.8',
	        reverse: false
	    })
	    .on('start', function () {
	    	var $elem = $(elem);
	    	$elem.attr('src', $elem.data('src'));
	    	$elem.imagesLoaded().done( function( instance ) {
		 		$elem.removeClass('loading-ajax');
			});
	    })
	    .addTo(sm_controller)
	});

