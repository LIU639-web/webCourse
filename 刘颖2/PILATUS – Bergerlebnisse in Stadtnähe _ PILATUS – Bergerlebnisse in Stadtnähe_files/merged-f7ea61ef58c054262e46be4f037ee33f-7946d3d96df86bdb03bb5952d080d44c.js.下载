	 $('#arrival-nav').datepicker({
        dateFormat: "dd.mm.yy",
        dayNames: window['langDayNames_'+actLang],
        dayNamesMin: window['langDayNamesMin_'+actLang],
        firstDay: 1,
        minDate: 0,
        monthNamesShort: window['langMonthNamesShort_'+actLang],
        monthNames: window['langMonthNames_'+actLang],
        onClose: function( selectedDate ) {
            if(selectedDate.length != 0){
                $("#departure-nav").datepicker( "option", "minDate", selectedDate);
            }else{
                $("#departure-nav").datepicker( "option", "minDate", 1);
            }
            setTimeout(function(){
            	$('#main-navigation').find('li.show').removeClass('show');
            }, 350);
             $('#ui-datepicker-div').removeClass('arrival-nav');
        },
        onSelect: function(date){
            var date2 = $('#arrival-nav').datepicker('getDate');
            date2.setDate(date2.getDate()+1);
            $('#departure-nav').datepicker('setDate', date2);
        },
        beforeShow: function (input, inst) {
	        setTimeout(function () {
	        	var cssTop = parseInt(inst.dpDiv.css('top'))-44;
	        	var cssLeft = parseInt(inst.dpDiv.css('left'))-290;
	            inst.dpDiv.css({
	                top: cssTop,
	                left: cssLeft
	            }).addClass('arrival-nav');
	        }, 0);
	    }
    });
     $('#departure-nav').datepicker({
        dateFormat: "dd.mm.yy",
        dayNames: window['langDayNames_'+actLang],
        dayNamesMin: window['langDayNamesMin_'+actLang],
        firstDay: 1,
        minDate: 0,
       	monthNamesShort: window['langMonthNamesShort_'+actLang],
        monthNames: window['langMonthNames_'+actLang],
        onClose: function() {
        	setTimeout(function(){
            	$('#main-navigation').find('li.show').removeClass('show');
            }, 350);
            $('#ui-datepicker-div').removeClass('departure-nav');
        },
        beforeShow: function (input, inst) {
	        setTimeout(function () {
	        	var cssTop = parseInt(inst.dpDiv.css('top'))-85;
	        	var cssLeft = parseInt(inst.dpDiv.css('left'))-290;
	            inst.dpDiv.css({
	                top: cssTop,
	                left: cssLeft
	            }).addClass('departure-nav');
	        }, 0);
	    }
    });

