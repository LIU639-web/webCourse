	var $grid = $('.teaser-grid').isotope({
		itemSelector: '.teaser-grid-item',
	  	percentPosition: true,
		stamp: '.stamp',
	  	masonry: {
	    // use outer width of grid-sizer for columnWidth
	    columnWidth: ($('.teaser-grid-item.columns.medium-6.large-3').length > 0 ? '.teaser-grid-item.columns.medium-6.large-3' : '.teaser-grid-item.columns.medium-6')
	  	}
