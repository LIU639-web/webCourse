moodparallax=function(){var e=this,n=null,t={x:0,y:0},i=$("#mood-slider").innerWidth()/2,o=$("#mood-slider").innerHeight()/2;e.init=function(){$("#mood-slider").mousemove(function(i){t.x=i.pageX,t.y=i.pageY,cancelAnimationFrame(n),n=requestAnimationFrame(e.update)}),$(window).resize(function(){e.cx=window.innerWidth/2,e.cy=window.innerHeight/2})},e.update=function(){var e=t.x-i,n=t.y-o,a=n/o*-1,r=-(e/i)*-1;a<-1&&(a=-1),a>1&&(a=1),r<-1&&(r=-1),r>1&&(r=1);var d=Math.sqrt(Math.pow(a,2)+Math.pow(r,2)),c=20*d;TweenLite.to(".slick-current .slider-item-content",1,{transform:"rotate3d("+a/2+", "+r/2+", 0, "+c+"deg) perspective(1000px)",ease:Power2.easeOut})},e.init()},moodcontentparallax=function(){var e=this,n=null,t={x:0,y:0},i=$("#content-mood").innerWidth()/2,o=$("#content-mood").innerHeight()/2;e.init=function(){$("#content-mood").mousemove(function(i){t.x=i.pageX,t.y=i.pageY,cancelAnimationFrame(n),n=requestAnimationFrame(e.update)}),$(window).resize(function(){e.cx=window.innerWidth/2,e.cy=window.innerHeight/2})},e.update=function(){var e=t.x-i,n=t.y-o,a=n/o*-1,r=-(e/i)*-1;a<-1&&(a=-1),a>1&&(a=1),r<-1&&(r=-1),r>1&&(r=1);var d=Math.sqrt(Math.pow(a,2)+Math.pow(r,2)),c=20*d;TweenLite.to("#content-mood .slider-item-content",1,{transform:"rotate3d("+a/2+", "+r/2+", 0, "+c+"deg) perspective(1000px)",ease:Power2.easeOut})},e.init()};
moodparallaxmobile=function(){var e=this,t=null,n={x:0,y:0,z:0};e.init=function(){window.addEventListener("deviceorientation",function(o){n.x=Math.round(o.beta),n.y=Math.round(o.gamma),cancelAnimationFrame(t),t=requestAnimationFrame(e.update)})},e.update=function(){window.matchMedia("(orientation: portrait)").matches?(n.x<-30&&(n.x=-30),n.x>30&&(n.x=30),n.y<-40&&(n.y=-40),n.y>40&&(n.y=40),TweenLite.to(".slick-current .slider-item-content",1,{transform:"rotateX("+n.x/2+"deg) rotateY("+n.y/2+"deg) rotateZ(0deg) perspective(1000px)",ease:Power2.easeOut})):(n.y<-30&&(n.y=-30),n.y>30&&(n.y=30),n.x<-40&&(n.x=-40),n.x>40&&(n.x=40),TweenLite.to(".slick-current .slider-item-content",1,{transform:"rotateX("+n.y/2+"deg) rotateY("+n.x/2+"deg) rotateZ(0deg) perspective(1000px)",ease:Power2.easeOut}))},e.init()},moodcontentparallaxmobile=function(){var e=this,t=null,n={x:0,y:0,z:0};e.init=function(){window.addEventListener("deviceorientation",function(o){n.x=Math.round(o.beta),n.y=Math.round(o.gamma),cancelAnimationFrame(t),t=requestAnimationFrame(e.update)})},e.update=function(){window.matchMedia("(orientation: portrait)").matches?(n.x<-30&&(n.x=-30),n.x>30&&(n.x=30),n.y<-40&&(n.y=-40),n.y>40&&(n.y=40),TweenLite.to("#content-mood .slider-item-content",1,{transform:"rotateX("+n.x/2+"deg) rotateY("+n.y/2+"deg) rotateZ(0deg) perspective(1000px)",ease:Power2.easeOut})):(n.y<-30&&(n.y=-30),n.y>30&&(n.y=30),n.x<-40&&(n.x=-40),n.x>40&&(n.x=40),TweenLite.to("#content-mood .slider-item-content",1,{transform:"rotateX("+n.y/2+"deg) rotateY("+n.x/2+"deg) rotateZ(0deg) perspective(1000px)",ease:Power2.easeOut}))},e.init()};
var rs_img_size = 'thumbnail';
var rs_container_id = null;
var rs_image_link = null;

function fitSize(image, size)
{
	if (!size || size <= 150) {
		return image;
	}

	newSize = '';
	if (size <= 256) {
		newSize = 'oneeighth';
	}
	else if (size <= 300) {
		newSize = 'optional';
	}
	else if (size <= 450) {
		newSize = 'medium';
	}
	else if (size <= 512) {
		newSize = 'quarter';
	}
	else if (size <= 800) {
		newSize = 'default';
	}
	else if (size <= 1024) {
		newSize = 'half';
	}
	else {
		newSize = 'full';
	}

    rs_img_size = newSize;

	image = image.replace (new RegExp('thumbnail$'), newSize);

	return image;
}

var loadJSONP = function ( url, callback ) {

    // // Create script with url and callback (if specified)
    // var ref = window.document.getElementsByTagName( 'script' )[ 0 ];
    // var script = window.document.createElement( 'script' );
    // script.src = url + (url.indexOf( '?' ) + 1 ? '&' : '?') + 'callback=' + callback;

    // // Insert script tag into the DOM (append to <head>)
    // ref.parentNode.insertBefore( script, ref );

    // // After the script is loaded (and executed), remove it
    // script.onload = function () {
    //     this.remove();
    // };

};

function parseSettings ( data ) {
    if (data.settings.cam_inactive===true)
        window.roundshot.offlineCamera(data.settings.offline_icon_color, data.hash, data.id);
};

(function() {
    var lastTime = 0;

    if (!window.requestAnimationFrameN)
        window.requestAnimationFrameN = function(callback, element) {
            var currTime = Date.now();
            var timeToCall = 20;
            var id = window.setTimeout(function() { callback(currTime + timeToCall); },
              timeToCall);
            lastTime = currTime + timeToCall;
            return id;
        };

    if (!window.cancelAnimationFrame)
        window.cancelAnimationFrame = function(id) {
            clearTimeout(id);
        };
}());

(function(){
	if(window.roundshot) return; // already included, abort

	var that = null;
	var speed = 1;

	window.roundshot = {
		queue: [],
		stamp: 0,
		frame: 0,

		animateBouncingItem: function(item) {
			var c_width = item.container.clientWidth;
			var img 	= item.container.childNodes[0];
			var i_width = img.clientWidth;
			var left 	= parseFloat(img.style.left);
			var margin  = Math.abs(c_width - i_width);

			if(c_width > i_width) {
				left = (c_width - i_width)/2;
			} else {
				left += (item.direction?-1:1)*speed*(that.frame/50);

				if(left < -margin) {
					left = -margin;
					item.direction = !item.direction;
				} else if(left > 0) {
					left = 0;
					item.direction = !item.direction;
				}
			}
			img.style.left = left +"px";
		},

		animateRotatingItem: function(item) {
			var img_1 	= item.container.childNodes[0];

			if (img_1)
				var i_width = img_1.clientWidth;
			else
				var i_width = 150;
			var left 	= parseFloat(img_1.style.left);

			left -= 1;

			if(left < -i_width) {
				left += (i_width-1);
			}
			for(var i=0; i<item.container.childNodes.length; i++) {
				var img_1 = item.container.childNodes[i];
				img_1.style.left = (left + i*(i_width-1))+"px";
			}
		},

		animateItem: function(item){
			if(item.bounce) {
				that.animateBouncingItem(item);
			} else {
				that.animateRotatingItem(item);
			}
		},

		animateQueue: function(stamp) {
			setTimeout(function(){requestAnimationFrameN(that.animateQueue);}, 20);

			that.frame = parseInt(stamp - that.stamp);
			if(that.stamp > 0) {
				for(var i=0; i < window.roundshot.queue.length; i++) {
					that.animateItem(window.roundshot.queue[i]);
				}
			}
			that.stamp = stamp;
		},

        offlineCamera: function(offline_icon_color, hash, id) {
            if (!offline_icon_color)
                offline_icon_color = 'red';
            var randomId = Math.floor(Math.random()*1000000);
            //var imgContainer = document.getElementById(rs_container_id);
            var imgContainer = document.getElementsByClassName("rs-img-"+id)[0];
            if (!imgContainer)
		    	imgContainer = document.getElementsByClassName("rs-img-"+hash)[0];
            var size = imgContainer.className.split('size-');
            if (size && size[1])
                rs_img_size = size[1];
            var img_id = "rs_offline_icon_" + randomId;
            var img = document.createElement('img');
            img.id = img_id;
            img.src = rs_image_link + "/img/offline-" + offline_icon_color + "-" + rs_img_size + ".png";
            img.style.position = "absolute";
            img.style.left = "50%";
            img.style.top = "50%";
            img.style.display = "none";
            imgContainer.appendChild(img);

            setTimeout(
                function () {
                    var _img = document.getElementById(img_id);
                    var margin_left = (_img.width/2);
                    var margin_top = (_img.height/2);
                    _img.style.marginLeft = "-" + margin_left + "px";
                    _img.style.marginTop = "-" + margin_top + "px";
                    _img.style.display = "block";
                },
                200
            );

        },

		animate: function(image) {
			//{picture: 'tutaj link do obrazka, url do img bezpoÅ›rednio',
			// angle: 'kÄ…t w stopniach'}
            var randomId = Math.floor(Math.random()*1000000);
            var split_img = image.picture.split("/");
			var container_id = 'roundshot_container_' + randomId;
			rs_container_id = container_id;
			var imglink_id = 'roundshot_imglink_' + randomId;
            var target = image.open_in_new_tab ? '_blank' : '_self';
			// document.write('<style>#'+imglink_id+' img { max-width: none !important; }</style><div style="position:relative" class="rs-img-'+split_img[4]+'" id="'+container_id+'"><a target="'+target+'" style="display:block;" id="'+imglink_id+'"></a></div>');

			if(typeof(document.getElementById('roundshotesel')) != 'undefined' && document.getElementById('roundshotesel') != null){
				document.getElementById('roundshotesel').innerHTML = '<style>#'+imglink_id+' img { max-width: none !important; }</style><div style="position:relative" class="rs-img-'+split_img[4]+'" id="'+container_id+'"><a target="'+target+'" style="display:block;" id="'+imglink_id+'"></a></div>';
			}

			if(typeof(document.getElementById('roundshoteselnolink')) != 'undefined' && document.getElementById('roundshoteselnolink') != null){
				document.getElementById('roundshoteselnolink').innerHTML = '<style>#'+imglink_id+' img { max-width: none !important; }</style><div style="position:relative" class="rs-img-'+split_img[4]+'" id="'+container_id+'"><a class="yay"  style="display:block;" id="'+imglink_id+'"></a></div>';
			}



			var container = document.getElementById(rs_container_id);
            var imglink = document.getElementById(imglink_id);


			if (image.width && isNaN(image.width)) {

				container.style.width    = image.width;
			} else {
				container.style.width    = image.width?image.width+"px":"720px";
			}

			if (image.height && isNaN(image.height)) {
				container.style.height   = image.height;
			} else {
				container.style.height   = image.height?image.height+"px":"150px";
			}



			container.style.overflow = "hidden";
			//container.style.margin   = "auto";
			container.style.position = "relative";

			if(typeof(document.getElementById('roundshotesel')) != 'undefined' && document.getElementById('roundshotesel') != null) {
				imglink.href = image.link;
			}

			var img = document.createElement('img');
			image.picture = fitSize(image.picture, image.height);
            container.className += ' size-' + rs_img_size;
			img.src                  = image.picture;
			img.style.height         = "100%";
			img.style.position       = "absolute";
			img.style.left 			 = 0;
			img.style.top 			 = 0;
			img.style.visiblity 	 = "hidden";
            img.style.border         = "none";
			if(navigator.userAgent.toLowerCase().indexOf('safari') != -1 && navigator.userAgent.toLowerCase().indexOf('chrome')!=-1)
				img.style.WebkitFilter      = "blur(0.2px)";

			img.onload = function () {

				img.style.visibility = "show";

				if(!image.bounce) {
					var width = img.clientWidth;
					var containerWidth = container.clientWidth;

					var it = 1;

					while(width <= (containerWidth + img.clientWidth)) {
						var _img = document.createElement('img');
						_img.src = image.picture;
						_img.style.height         = "100%";
						_img.style.position       = "absolute";
						_img.style.left 		  = 0;
						_img.style.top 			  = 0;
                        _img.style.border         = "none";
						if(navigator.userAgent.toLowerCase().indexOf('safari') != -1 && navigator.userAgent.toLowerCase().indexOf('chrome')!=-1)
							_img.style.WebkitFilter      = "blur(0.2px)";

						width += img.width;
						imglink.appendChild(_img);
						imglink.appendChild(_img);
						if (it>20) break;
						it++;
					}
				}

				that.queue.push({'container': imglink,
					 'direction': true,
					 'bounce': image.bounce});
			};

			imglink.appendChild(img);

            //rs_image_link = image.link;

            var split = image.link.split("/");

            rs_image_link = split[0] + "/" + split[1] + "/" + split[2] + "/";

            if (split.length>3) {
                loadJSONP( split[0] + "/" + split[1] + "/" + split[2] + "/app_front.php/settings.min.json/" + split[3] + "/", 'parseSettings' );
            } else
                loadJSONP( image.link + "settings.min.json", 'parseSettings' );
		
		}
	};

	that = window.roundshot;

	requestAnimationFrameN(that.animateQueue);
})();


themap = function () {
	var self = this;
	var panZoomMap;
	var circleSize;
	var initMapX;
	var initMapY;
	var initMapZoom;
	var initMapFilter;
	var pathname = window.location.pathname;

	self.init = function(){



		self.circleSize = 0;

		if( self.getQueryVariable('mapfilter') ){
			if(self.getQueryVariable('mapfilter').toString().indexOf(',') !== -1){
				self.initMapFilter = self.getQueryVariable('mapfilter').split(',');
			}else{
				self.initMapFilter = [self.getQueryVariable('mapfilter')];
			}
		}

		else if( $('#map-param').length > 0 ){
			if($('#map-param').data('filter').toString().indexOf(',') !== -1){
				self.initMapFilter = $('#map-param').data('filter').split(',');
			}else{
				self.initMapFilter = [$('#map-param').data('filter')];
			}
		}




		if(pathname == '/pilatus-karte-test'){
			self.panZoomMap = svgPanZoom('#themap', {
				viewportSelector: '.svg-pan-zoom_viewport'
				, panEnabled: true
				, controlIconsEnabled: false
				, zoomEnabled: true
				, dblClickZoomEnabled: true
				, mouseWheelZoomEnabled: true
				, preventMouseEventsDefault: true
				, zoomScaleSensitivity: 0.2
				, minZoom: 0.5
				, maxZoom: 5
				, fit: true
				, contain: false
				, center: true
				, refreshRate: 'auto'
				, beforeZoom: function(zoom){
					if(self.circleSize == 0){
						self.circleSize = $('.map-marker .map-marker-fg')[0].getBoundingClientRect().width / zoom;
					}
				}
				, onZoom: function(zoom){
					// if( navigator.userAgent.toLowerCase().indexOf('firefox') == -1 ){
					var sizes = this.getSizes();
					var korrektur = 40 / (24 * sizes.realZoom);
					$('#themap').find('.map-marker-scale').attr('transform','scale('+korrektur+')');
					// }

					setTimeout(function(){
						self.setTooltipMarker();
					}, 50);
				}
				, beforePan: self.beforePan
				, onPan: function(){
					self.setTooltipMarker();
				}
				, onUpdatedCTM: function(){}
				, customEventsHandler: self.eventsHandler
				, eventsListenerElement: null
			});
		} else {
			self.panZoomMap = svgPanZoom('#themap', {
				viewportSelector: '.svg-pan-zoom_viewport'
				, panEnabled: true
				, controlIconsEnabled: false
				, zoomEnabled: true
				, dblClickZoomEnabled: true
				, mouseWheelZoomEnabled: true
				, preventMouseEventsDefault: true
				, zoomScaleSensitivity: 0.2
				, minZoom: 0.5
				, maxZoom: 5
				, fit: true
				, contain: false
				, center: true
				, refreshRate: 'auto'
				, beforeZoom: function(zoom){
					if(self.circleSize == 0){
						self.circleSize = $('.map-marker .map-marker-fg')[0].getBoundingClientRect().width / zoom;
					}
				}
				, onZoom: function(zoom){
					// if( navigator.userAgent.toLowerCase().indexOf('firefox') == -1 ){
					var sizes = this.getSizes();
					var korrektur = 40 / (24 * sizes.realZoom);
					$('#themap').find('.map-marker-scale').attr('transform','scale('+korrektur+')');
					// }

					setTimeout(function(){
						self.setTooltipMarker();
					}, 50);
				}
				, beforePan: self.beforePan
				, onPan: function(){
					self.setTooltipMarker();
				}
				, onUpdatedCTM: function(){}
				, customEventsHandler: self.eventsHandler
				, eventsListenerElement: null
			});
		}
		setTimeout(function(){

			if(self.initMapFilter){
				$('#map-filter-bar').find('input:checked').next().click();
				$.each(self.initMapFilter, function( index, value ) {
					$('#map-filter-bar').find('label[for=filter-'+value+']').click();
				});
			}
			self.panZoomMap.resize(); // update SVG cached size and controls positions
			self.panZoomMap.fit();
			self.panZoomMap.center();

			self.panZoomMap.zoomIn();
			self.panZoomMap.zoomOut();

			$(window).scrollTop(0);
			if( document.getElementById('themap') != null){
				self.removeLoaderContent();
			}

		}, 250);

		self.setTooltipMarker();

		// $(window).resize(function(){
		// 	self.panZoomMap.resize();
		// 	self.panZoomMap.center();
		// })

		$('#map-control-reset').click(function(e){
			e.preventDefault();
			self.panZoomMap.resize();
			self.panZoomMap.resetZoom();
			self.panZoomMap.center();
		});
		$('#map-control-zoom-in').click(function(e){
			e.preventDefault();
			self.panZoomMap.zoomIn();
		});
		$('#map-control-zoom-out').click(function(e){
			e.preventDefault();
			self.panZoomMap.zoomOut();
		});

		$('#open-map-filter').click(function(e){
			e.preventDefault();
			$('#map-filter-bar').removeClass('hidden').addClass('show');
			setTimeout(function(){
				$('#map').removeClass('filter-bar-closed');
				self.panZoomMap.center();
			}, 250);

		});

		$('#map-filter-bar-close').click(function(e){
			e.preventDefault();
			$('#map-filter-bar').addClass('hidden').removeClass('show');
			$('#map').addClass('filter-bar-closed');
			self.panZoomMap.center();
		});

		$('#close-themap').click(function(e){
			e.preventDefault();
			if( $('body').hasClass('map') ){
				window.location =  $(this).data('url');
			}else{
				// console.log('close the map')
				if( !!(window.history && history.pushState) ){
					window.history.back();
				}else{
					$('#map-modal').foundation('close');
					$('body').removeClass('map-is-open');
				}
			}
		});

		$('.map-marker-fg').mouseenter(function() {
			if(!Modernizr.touchevents){
				var $this = $(this);
				var markerOffset = $this.offset();
				var $mapToolTip = $('#'+ $this.parents('.map-marker').data('tooltipid') );

				var toolTipY = markerOffset.top - $mapToolTip.height() - 35;
				var toolTipX = markerOffset.left - ($mapToolTip.outerWidth()/2) + ($this[0].getBoundingClientRect().width/2);

				$mapToolTip.css({
					top: toolTipY,
					left: toolTipX
				}).addClass('showTooltip');
			}

		});

		$('.map-marker').mouseleave(function() {
			self.hideAllMarker();
		});

		$('#map-filter-bar').find('input[type=checkbox]').change(function(e){
			$this = $(this);

			if( $this.is(':checked') ){
				$('#themap').find("g[data-filter='"+$this.attr('id')+"']").addClass('show');

				$('#themap').find("g[data-filter='"+$this.attr('id')+"']").each(function(){
					var toolTipId = $(this).data('tooltipid');
					$('#'+toolTipId).removeClass('deactivated');
				})
			}else{
				$('#themap').find("g[data-filter='"+$this.attr('id')+"']").removeClass('show');
				$('#themap').find("g[data-filter='"+$this.attr('id')+"']").each(function(){
					var toolTipId = $(this).data('tooltipid');
					$('#'+toolTipId).addClass('deactivated');
				})
			}
		})

		$('.reveal').foundation();

		$('.reveal-in-map').on('open.zf.reveal', function(e) {
			var $imgTarget = $(e.currentTarget).find('img.loading-image');

			if($imgTarget.length > 0){
				// console.log($imgTarget);
				if($imgTarget.hasClass('map-no-cache')){
					var imgSrc = [$imgTarget.data('src') + '?t=' + Date.now()];
				}else{
					var imgSrc = [$imgTarget.data('src')];
				}

				var imgLoad = imagesLoaded(imgSrc);
				imgLoad.on('done', function(instance,image){
					$imgTarget.attr('src', imgSrc).removeClass('loading-image');
				});
			}

			var $iframeTarget = $(e.currentTarget).find('iframe.loading-iframe');
			if($iframeTarget.length > 0){
				var iFrameSrc = $iframeTarget.data('src');
				$iframeTarget.attr('src', iFrameSrc).removeClass('loading-iframe');
			}
		});

		$('#modal-content-37').on('open.zf.reveal', function() {
			// roundshot
			roundshot.animate({ picture: "//backend.roundshot.com/cams/155/default", link: "//pilatus.roundshot.com/", angle: 360, width: 'auto', height: 350, bounce: false, open_in_new_tab: true })
		});

	}

	self.beforePan = function(oldPan, newPan){

		var stopHorizontal = true
		, stopVertical = true
		, gutterWidth = 200
		, gutterHeight = 200
		// Computed variables
		, sizes = this.getSizes()
		, leftLimit = -((sizes.viewBox.x + sizes.viewBox.width) * sizes.realZoom) + gutterWidth
		, rightLimit = sizes.width - gutterWidth - (sizes.viewBox.x * sizes.realZoom)
		, topLimit = -((sizes.viewBox.y + sizes.viewBox.height) * sizes.realZoom) + gutterHeight
		, bottomLimit = sizes.height - gutterHeight - (sizes.viewBox.y * sizes.realZoom)

		customPan = {}
		customPan.x = Math.max(leftLimit, Math.min(rightLimit, newPan.x))
		customPan.y = Math.max(topLimit, Math.min(bottomLimit, newPan.y))
		console.log(customPan);
		return customPan
	}

	self.eventsHandler = {
		haltEventListeners: ['touchstart', 'touchend', 'touchmove', 'touchleave', 'touchcancel']
		, init: function(options) {

			var instance = options.instance
				, initialScale = 1
				, pannedX = 0
				, pannedY = 0

			// Init Hammer
			// Listen only for pointer and touch events
			this.hammer = Hammer(options.svgElement, {
				inputClass: Hammer.SUPPORT_POINTER_EVENTS ? Hammer.PointerEventInput : Hammer.TouchInput
			})

			// Enable pinch
			this.hammer.get('pinch').set({enable: true})

			// Handle double tap
			this.hammer.on('doubletap', function(ev){
				instance.zoomIn()
			})

			// Handle pan
			this.hammer.on('panstart panmove', function(ev){
				// On pan start reset panned variables
				if (ev.type === 'panstart') {
					pannedX = 0
					pannedY = 0
				}

				// Pan only the difference
				instance.panBy({x: ev.deltaX - pannedX, y: ev.deltaY - pannedY})
				pannedX = ev.deltaX
				pannedY = ev.deltaY
			})

			// Handle pinch
			this.hammer.on('pinchstart pinchmove', function(ev){
				// On pinch start remember initial zoom
				if (ev.type === 'pinchstart') {
					initialScale = instance.getZoom()
					instance.zoom(initialScale * ev.scale)
				}
				instance.zoom(initialScale * ev.scale)
			})

			// Prevent moving the page on some devices when panning over SVG
			options.svgElement.addEventListener('touchmove', function(e){ e.preventDefault(); });
		}

		, destroy: function(){
			this.hammer.destroy()
		}
	}

	self.hideAllMarker = function(){
		$('#map').find('.map-tooltip').removeClass('showTooltip');
	}

	self.setTooltipMarker = function(){
		$('#map').find('.show-always-title:visible').each(function(){
			var $this = $(this);
			var markerOffset = $this.offset();
			var $mapToolTip = $('#'+ $this.parents('.map-marker').data('tooltipid') );

			var toolTipY = markerOffset.top - $mapToolTip.height() - 35;
			var toolTipX = markerOffset.left - ($mapToolTip.outerWidth()/2) + ($this[0].getBoundingClientRect().width/2);

			$mapToolTip.css({
				top: toolTipY,
				left: toolTipX
			});
		})
	}

	self.getQueryVariable = function(variable){
       var query = window.location.search.substring(1);
       var vars = query.split("&");
       for (var i=0;i<vars.length;i++) {
               var pair = vars[i].split("=");
               if(pair[0] == variable){return pair[1];}
       }
       return(false);
	}

	self.removeLoaderContent = function(){
		TweenLite.to('#dragon-loader-content', 0.4, {opacity:0, ease:Power2.easeInOut, onComplete: function(){
				TweenLite.to('#dragon-loader-container-content', 0.8, {opacity:0, ease:Power2.easeInOut, onComplete: function(){
					$('body').removeClass('loading-content');
				}});
			}
		});
	}

	self.init();
}

